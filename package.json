{
  "name": "n8n-nodes-docusign-esign",
  "version": "0.4.1",
  "description": "n8n community node for DocuSign eSignature - send documents for signature and manage envelopes",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "docusign",
    "esignature",
    "signature",
    "envelope",
    "documents",
    "contracts",
    "signing"
  ],
  "license": "MIT",
  "homepage": "https://github.com/janmaaarc/n8n-nodes-docusign-esign",
  "author": {
    "name": "Jan Marc Coloma",
    "email": "janmarccolomaaa@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/janmaaarc/n8n-nodes-docusign-esign.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials test --write",
    "format:check": "prettier nodes credentials test --check",
    "lint": "eslint nodes credentials --ext .ts",
    "lintfix": "eslint nodes credentials --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DocuSignApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/DocuSign/DocuSign.node.js",
      "dist/nodes/DocuSign/DocuSignTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.19.30",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.55.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
